{
  "name": "dashboard-iegm",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "# Development": "",
    "dev": "vite --port 3000",
    "dev:d1": "wrangler pages dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "yarn dev",
    "clean": "rm -rf node_modules/.vite dist",
    "# Database": "",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:drop": "drizzle-kit drop",
    "# Data": "",
    "data:import": "tsx scripts/import-data.ts",
    "data:migrate": "tsx scripts/import-data.ts",
    "data:seed": "tsx scripts/seed-data.ts",
    "data:reset": "rm -f local.db && yarn db:migrate && yarn data:seed && yarn data:import",
    "# Cloudflare": "",
    "cf:deploy": "wrangler pages deploy",
    "cf:db:create": "wrangler d1 create iegm-dashboard",
    "cf:db:studio": "wrangler d1 studio iegm-dashboard",
    "# Migration": "",
    "migrate:local": "tsx scripts/migrate-local.ts",
    "migrate:prod": "tsx scripts/migrate-prod.ts",
    "# Setup": "",
    "setup": "yarn install && yarn db:generate && yarn db:migrate && yarn data:seed && yarn data:migrate",
    "# Testing": "",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:api": "tsx scripts/test-api.ts",
    "# Code Quality": "",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --build"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250709.0",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@tanstack/vue-query": "^5.83.0",
    "@vueuse/core": "^13.5.0",
    "axios": "^1.10.0",
    "better-sqlite3": "^12.2.0",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.0.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.2",
    "lucide-vue-next": "^0.525.0",
    "papaparse": "^5.5.3",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@tailwindcss/vite": "^4",
    "@tsconfig/node22": "^22.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.12",
    "@types/papaparse": "^5.3.16",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vitest/eslint-plugin": "^1.2.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "3.5.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  },
  "packageManager": "yarn@4.9.2"
}
